        "Mask",
        "Awakening",
        "Ornament",
        "Breath Amplification"
    },
    Multi = false,
    Default = 1
})

MachineDropdown:OnChanged(function(value)
    if value == "Select Machine" then return end
    var.machine.s = value
end)

