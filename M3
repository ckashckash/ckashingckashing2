if allowedUsers[player.Name] then

    -- Toggle for skill 200563
    Tabs.Main:AddToggle("SkillSpamToggle", {
        Title = "ADD DAMAGE",
        Default = false,
        Callback = function(state)
            spamEnabled = state
        end
    })

    -- Toggle for skill 200314
    Tabs.Main:AddToggle("SkillSpamToggle2", {
        Title = "RAPID SKILL",
        Default = false,
