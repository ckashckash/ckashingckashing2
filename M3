    local rarityToggle = Tabs.Machine:AddToggle("Keep_" .. rarity, {
        Title = "Keep " .. rarity:sub(1,1):upper() .. rarity:sub(2),
        Default = false
    })

    rarityToggle:OnChanged(function(value)
        var.machine.delete[rarity] = value
    end)
end

