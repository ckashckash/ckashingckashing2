-- Utils for notification
local utils = {
    notify = function(msg, duration)
        print("[Notify]", msg)
    end,
    ColorFonts = function(text, color)
        return text .. " (" .. color .. ")"
    end,
    runtime = function(callback)
        task.spawn(function()
            while true do
                task.wait(1)
                callback()
            end
        end)
    end
}

-- Main state table
local var = {
    machine = {
        s = "Select Machine",
        toggle = false,
        speed = 0.8,
        table = { "Mask", "Breath", "Ornament", "Breath Amplification" },
        delete = {
            common = false,
            rare = false,
            epic = false,
            legendary = false,
            mythic = false
        }
    }
}

