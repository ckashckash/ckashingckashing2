                    if spamEnabled then
                        HeroSkillHarm:FireServer({
                            harmIndex = 1,
                            isSkill = true,
                            heroGuid = guid,
                            skillId = 200408
                        })
                    end
                    if spamEnabled2 then
                        HeroSkillHarm:FireServer({
                            harmIndex = 1,
                            isSkill = true,
                            heroGuid = guid,
                            skillId = 200314
                        })
                    end
                end
            end
            task.wait(0.00001)
        end
    end)
end


local ReplicatedStorage = game:GetService("ReplicatedStorage")
local Players = game:GetService("Players")
local LocalPlayer = Players.LocalPlayer

local rerollRemote = ReplicatedStorage:WaitForChild("Remotes"):WaitForChild("RerollOrnament")
local drawId = 400001 -- Mask
local spinning = false
local spinDelay = 0.1
local selectedOrnaments = {}
local ornamentToggles = {}

-- Mask ornaments
local maskOrnaments = {
    {Id = 410004, Name = "Fire Man Mask"},
    {Id = 410005, Name = "Tengu Mask"},
    {Id = 410050, Name = "Red Horn"},
    {Id = 410051, Name = "Six-Eye Violet"},
    {Id = 410052, Name = "Fox Tears"}
}

local allowedUsers = {"Cuteeee1587", "lxxSAWxxI"}
if not table.find(allowedUsers, LocalPlayer.Name) then return end

-- Helper
local function tableHasValue(tbl, val)
    for _, v in ipairs(tbl) do
        if tonumber(v) == tonumber(val) then
            return true
        end
    end
    return false
end

-- Create tab
local Tab = Window:AddTab({Title = "Mask Reroll", Icon = "swords"})
Tab:AddSection("MaskToggles", {Title = "Select Mask Ornaments to Auto-Stop"})

-- Add toggles for each mask ornament
for _, ornament in ipairs(maskOrnaments) do
    local toggle = Tab:AddToggle({
        Title = ornament.Name,
        Section = "MaskToggles",
        Default = false
    })
    toggle:OnChanged(function(state)
        if state then
            table.insert(selectedOrnaments, ornament.Id)
        else
            for i, v in ipairs(selectedOrnaments) do
                if v == ornament.Id then
                    table.remove(selectedOrnaments, i)
                    break
                end
            end
        end
    end)
    table.insert(ornamentToggles, toggle)
end

-- Spin speed slider
Tab:AddSlider("SpinSpeed", {
    Title = "Spin Speed",
    Description = "Delay between rerolls (lower = faster)",
    Default = 0.1,
    Min = 0.01,
    Max = 1,
    Rounding = 2,
    Callback = function(value)
        spinDelay = value
    end
})

-- Spin loop
local function spinLoop()
    while spinning do
        local success, result = pcall(function()
            return rerollRemote:InvokeServer(drawId)
        end)

        if success and result and result.ornamentId then
            local ornamentName = nil
            for _, o in ipairs(maskOrnaments) do
                if o.Id == result.ornamentId then
                    ornamentName = o.Name
                    break
                end
            end

            Fluent:Notify({
                Title = "Reroll Result",
                Content = "Rolled Ornament: " .. (ornamentName or tostring(result.ornamentId)),
                Duration = 2
            })

            if tableHasValue(selectedOrnaments, result.ornamentId) then
                Fluent:Notify({
                    Title = "Success",
                    Content = "üéØ Got desired ornament: " .. (ornamentName or tostring(result.ornamentId)),
                    Duration = 3
                })
                spinning = false
                break
            end
        end

        task.wait(spinDelay)
    end
end

-- Start button
Tab:AddButton({
    Title = "Start Reroll",
    Description = "Begin auto rerolling mask ornaments",
    Callback = function()
        if spinning then return end
        spinning = true
        task.spawn(spinLoop)
        Fluent:Notify({Title = "Started", Content = "Mask auto reroll started!", Duration = 2})
    end
})

-- Stop button
Tab:AddButton({
    Title = "Stop Reroll",
    Description = "Stop the reroll loop",
    Callback = function()
        spinning = false
        Fluent:Notify({Title = "Stopped", Content = "Mask auto reroll stopped!", Duration = 2})
    end
})


