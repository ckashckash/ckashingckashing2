Tabs.Farm:AddButton({
    Title = "Destroy PopUp Notification",
    Description = nil,
    Callback = function()
        local playerGui = game:GetService("Players").LocalPlayer:WaitForChild("PlayerGui")
        local tipsPanel = playerGui:FindFirstChild("TipsPanel")

        if tipsPanel then
            local rewardsFrame = tipsPanel:FindFirstChild("RewardsFrame")
            if rewardsFrame then
                rewardsFrame:Destroy()
                print("Deleted RewardsFrame.")
            else
                print("RewardsFrame not found.")
            end

            local floatingTextFrame = tipsPanel:FindFirstChild("FloatIngTextFrame")
            if floatingTextFrame then
                floatingTextFrame:Destroy()
                print("Deleted FloatIngTextFrame.")
            else
                print("FloatIngTextFrame not found.")
            end
        else
            print("TipsPanel not found.")
        end
    end
})














-- Ornament Spinner Tabs (all users)
-------------------------------------------------- Auto Spin Mask
local Tab09 = Window:AddTab({ Title = "Auto Spin Mask", Icon = "crown" })
local targetOrnaments = {}
local spinning = false
local spinDelay = 0.1

local ReplicatedStorage = game:GetService("ReplicatedStorage")
local RerollOrnament = ReplicatedStorage:WaitForChild("Remotes"):WaitForChild("RerollOrnament")

local ornamentList = {
    ["Fire Man Mask"] = 410004,
    ["Tengu Mask"] = 410005,
    ["Red Horn"] = 410050,
    ["Six-Eye Violet"] = 410051,
    ["Fox Tears"] = 410052
}

local function tableHasValue(tbl, val)
    for _, v in ipairs(tbl) do
        if tonumber(v) == tonumber(val) then
            return true
        end
    end
    return false
end

local function spinLoop(argsId, targets)
    while spinning do
        local args = {argsId}
        local result = RerollOrnament:InvokeServer(unpack(args))

        if tableHasValue(targets, result.ornamentId) then
            Fluent:Notify({Title = "Success", Content = "üéØ Rolled target ornament!: " .. tostring(result.ornamentId), Duration = 3})
            spinning = false
            break
        else
            Fluent:Notify({Title = "Ornament Roll", Content = "Rolled: " .. tostring(result.ornamentId), Duration = 1})
        end
        task.wait(spinDelay)
    end
end

-- Toggles
for label, id in pairs(ornamentList) do
    local check = Tab09:AddToggle(label, {Title = label, Default = false})
    check:OnChanged(function(state)
        if state then
            table.insert(targetOrnaments, id)
        else
            for i, v in ipairs(targetOrnaments) do
                if v == id then table.remove(targetOrnaments, i) break end
            end
        end
    end)
end

-- Spin Speed Slider
Tab09:AddSlider("SpinSpeed", {Title = "Spin Speed", Description = "Delay between rolls (lower = faster)", Default = 0.1, Min = 0.01, Max = 1, Rounding = 2, Callback = function(value) spinDelay = value end})

-- Start/Stop Buttons
Tab09:AddButton({Title = "Start Rolling", Description = "Begin auto ornament rolling", Callback = function()
    if spinning then return end
    if #targetOrnaments == 0 then
        Fluent:Notify({Title = "Error", Content = "No ornaments selected!", Duration = 3})
        return
    end
    spinning = true
    task.spawn(function() spinLoop(400001, targetOrnaments) end)
    Fluent:Notify({Title = "Started", Content = "Ornament auto roll started!", Duration = 3})
end})

Tab09:AddButton({Title = "Stop Rolling", Description = "Stop the roll loop", Callback = function()
    spinning = false
    Fluent:Notify({Title = "Stopped", Content = "Ornament auto roll stopped!", Duration = 3})
end})

-------------------------------------------------- Auto Spin Stripe
